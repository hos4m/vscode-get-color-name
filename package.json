{
	"name": "vscode-get-color-name",
	"displayName": "VSCode Extension: Get Color Name",
	"description": "A VSCode extension to get the name of the color from its HEX value",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.getColorName"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.getColorName",
				"title": "Get Color Name"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"axios": "^0.18.0"
	}
}
